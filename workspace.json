{
  "version": 2,
  "projects": {
    "airkit": {
      "root": "apps/airkit",
      "projectType": "application",
      "sourceRoot": "apps/airkit/src",
      "targets": {
        "build": {
          "executor": "@nxext/sveltekit:sveltekit",
          "options": {
            "command": "build"
          }
        },
        "serve": {
          "executor": "@nxext/sveltekit:sveltekit",
          "options": {
            "command": "dev"
          }
        },
        "add": {
          "executor": "@nxext/sveltekit:add"
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/airkit/**/*.{ts,svelte,spec.ts}"]
          }
        }
      }
    },
    "airtable-cache": {
      "root": "packages/airtable-cache",
      "sourceRoot": "packages/airtable-cache/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/airtable-cache",
            "main": "packages/airtable-cache/src/index.ts",
            "tsConfig": "packages/airtable-cache/tsconfig.lib.json",
            "assets": ["packages/airtable-cache/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/airtable-cache/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/airtable-cache"],
          "options": {
            "jestConfig": "packages/airtable-cache/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "airtable-codegen": {
      "root": "packages/airtable-codegen",
      "sourceRoot": "packages/airtable-codegen/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/airtable-codegen",
            "main": "packages/airtable-codegen/src/index.ts",
            "tsConfig": "packages/airtable-codegen/tsconfig.lib.json",
            "assets": ["packages/airtable-codegen/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/airtable-codegen/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/airtable-codegen"],
          "options": {
            "jestConfig": "packages/airtable-codegen/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
